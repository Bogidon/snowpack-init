{
  "packageManifest": {
    "snowpack": {
      "webDependencies": [
        "vue/dist/vue.esm.browser.js",
        "http-vue-loader/src/httpVueLoader.js"
      ]
    },
    "scripts": {
      "prepare": "snowpack --clean",
      "start": "serve ./"
    }
  },
  "commands": {
    "install": {
      "exec": "npm install --silent --save http-vue-loader vue > /dev/null"
    },
    "install -D": {
      "exec": "npm install --silent -D snowpack serve > /dev/null"
    }
  }
}
